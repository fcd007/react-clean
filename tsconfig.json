{
    "compilerOptions": {
      "target": "es6",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "jsx": "react",
      "baseUrl":"src",
      "rootDir":"src",
      "paths": {
        "@/*": ["*"]
      }
    },
    "include": [
      "src"
    ]
  }
  